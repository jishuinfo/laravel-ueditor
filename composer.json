{
    "name": "jishuinfo/laravel-ueditor",
    "description": "UEditor integration for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "jishuinfo",
            "email": "langfangjishu@163.com"
        }
    ],
    "autoload": {
        "psr-4":{
            "Jishuinfo\\LaravelUEditor\\":"src/"
        }
    },
    "suggest": {
        "overtrue/laravel-filesystem-qiniu": "如果你想要使用七牛云存储，也许你需要安装它哦~"
    },
    "require": {
        "php": ">=7.3",
        "ext-fileinfo": "*",
        "illuminate/support": ">=6.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jishuinfo\\LaravelUEditor\\UEditorServiceProvider"
            ]
        }
    }
}
